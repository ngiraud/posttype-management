{
    "name": "ngiraud/posttype-management",
    "description": "Add or remove a simple posttype configuration to your Laravel application.",
    "keywords": [
        "ngiraud",
        "laravel",
        "posttype",
        "wordpress"
    ],
    "homepage": "https://github.com/ngiraud/posttype-management",
    "license": "MIT",
    "authors": [
        {
            "name": "Nicolas Giraud",
            "email": "contact@ngiraud.me"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0",
        "spatie/laravel-sluggable": "^2.1"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit" : "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "NGiraud\\PostType\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NGiraud\\PostType\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "NGiraud\\PostType\\PostTypeServiceProvider"
            ]
        }
    }
}
